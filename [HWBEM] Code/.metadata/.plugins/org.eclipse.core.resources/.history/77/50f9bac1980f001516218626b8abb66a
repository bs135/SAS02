#include "Input.h"

void Input_InitController(){
	/* Configure GPIO pin as input pin */
	Chip_GPIO_SetPinDIRInput(LPC_GPIO_BUTTON, BUTTON_PORT, BUTTON_UP_PIN);
	Chip_GPIO_SetPinDIRInput(LPC_GPIO_BUTTON, BUTTON_PORT, BUTTON_DOWN_PIN);

}
